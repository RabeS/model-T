<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.accommodationbackofficeservices.stocklevel.ManageAccommodationStockLevelInfo">
        <description>POJO containing info about the models and stock for which the create/update stock is to be performed.</description>
        <property name="accommodationOffering" type="de.hybris.platform.travelservices.model.warehouse.AccommodationOfferingModel"/>
        <property name="productType" type="java.lang.String"/>
        <property name="startDate" type="java.util.Date" />
        <property name="endDate" type="java.util.Date" />
        <property name="stockLevelAttributes" type="java.util.List&lt;de.hybris.platform.travelservices.stocklevel.StockLevelAttributes>"/>
        <property name="modified" type="java.lang.Boolean" />
    </bean>
    
</beans>
